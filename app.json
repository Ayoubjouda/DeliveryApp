{
  "expo": {
    "name": "MusicApp",
    "slug": "MusicApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/splach.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/img/splach.png",
      "resizeMode": "fill",
      "backgroundColor": "#1D272F"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/splach.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
